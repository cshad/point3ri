<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    @*<title>@ViewBag.Title</title>*@
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
    <meta name="description" content="rezervacija opreme na VERN-u">
    <meta name="keywords" content="rezervacija, oprema, stolni, tenis, laptop, printer, studenti, student, fakultet, vern">
    <meta name="author" content="templateninja, uređeno od strane vern grupe P31A 2015/2016">
    <link rel="shortcut icon" href="~/Content/Tema/img/favicon.ico">

    <title>Vern R</title>

    <!-- Bootstrap core CSS -->
    <link href="~/Content/Tema/css/bootstrap.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href='http://fonts.googleapis.com/css?family=Raleway:400,800' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Noto+Serif:700,400italic' rel='stylesheet' type='text/css'>
    <link href="~/Content/Tema/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/Tema/css/style.min.css" rel="stylesheet">
    <link href="~/Content/Tema/css/responsive.min.css" rel="stylesheet">
    <link href="~/Content/Tema/css/red.min.css" rel="stylesheet" id="color-style">
    <!-- begin:demo-css -->
    <link href="~/Content/Tema/"css/demo.css" rel="stylesheet">
    <!-- end:demo-css -->
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->

</head>
<body>
 
    <!-- begin:navbar -->
    <nav class="navbar navbar-default navbar-transparent navbar-fixed-top" role="navigation">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-top">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="~/Content/Tema/index.html"><i class="fa fa-recycle"></i> <span>Vern </span>R</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="navbar-top">
                <ul class="nav navbar-nav navbar-right">
                    <li class="active"><a href="~/Content/Tema/index.html">Početna</a></li>
                    <li><a href="~/Content/Tema/about.html">Rezervacije</a></li>
                    <li><a href="~/Content/Tema/contact.html">Obavijesti</a></li>
                    <li><a href="~/Content/Tema/page.html">O nama</a></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Profil <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="~/Content/Tema/blog.html">Prijašnje rezervacije</a></li>
                            <li><a href="~/Content/Tema/single.html">Postavke</a></li>
                            <li><a href="~/Content/Tema/archive.html">Admin panel</a></li>
                            <li><a href="~/Content/Tema/category.html">Odjavi se</a></li>
                            <!--<li><a href="~/Content/Tema/portfolio.html">Portfolio</a></li>
                            <li><a href="~/Content/Tema/portfolio-single.html">Portfolio Single</a></li>-->
                        </ul>
                    </li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-fluid -->
    </nav>
    <!-- end:navbar -->
    <!-- begin:header -->
    <section id="header" style="background-image: url(/Content/Tema/img/tt.jpg);">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <blockquote>
                        <p>Rezervacija termina za stolni tenis</p>
                        <small>Slobodno stolova: 2</small>
                    </blockquote>
                </div>
            </div>
        </div>
        <div class="overlay"></div>
    </section>
    <!-- end:header -->
    <!-- begin:featured -->
    <div id="featured">
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-sm-4">
                    <div class="featured-container">
                        <div class="featured-img">
                            <img src="~/Content/Tema/img/tth.jpg" alt="image post category danish">
                            <h3><a href="#">Stolni tenis</a></h3>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-4">
                    <div class="featured-container">
                        <div class="featured-img">
                            <img src="~/Content/Tema/img/lph.png" alt="image post category danish">
                            <h3><a href="#">Računala</a></h3>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-4">
                    <div class="featured-container">
                        <div class="featured-img">
                            <img src="~/Content/Tema/img/pth.jpg" alt="image post category danish">
                            <h3><a href="#">Printeri</a></h3>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end:featured -->


    <div>
        @RenderBody()
    </div>


</body>
</html>
